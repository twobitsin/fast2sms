{
    "name": "twobitsin/fast2sms",
    "description": "laravel notification implementation of fast2sms APIs.",
    "type": "library",
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/notifications": "^9.35",
        "illuminate/support": "^9.35"
    },
    "require-dev": {
       "mockery/mockery": "^1.4.4",
       "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "TwoBitsIn\\Fast2sms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TwoBitsIn\\Fast2sms\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "TwoBitsIn\\Fast2sms\\Fast2smsServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "TwoBits Technologies Pvt. Ltd.",
            "email": "dev@twobits.in"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
